extends Node2D
class_name player

# dynamic typing increases performance up to 200% and decreases memory used, because the 
# compiler doesnt need to find out which type the variable is.
# float numbers
var float_a: float = 1.532
var float_b: float = 2.34

# integer (int) numbers
var int_a: int = 2
var int_b: int = 1


# Called when the node enters the scene tree for the first time.
func _ready():
	print(float_a + float_b)
	print(int_a - int_b) 
